{
  "version": "0.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "setup": "yarn wsrun --exclude-missing --stages setup",
    "start": "yarn wsrun --exclude-missing start",
    "reset": "yarn wsrun --exclude-missing --stages reset",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/core": "^7.12.16",
    "babel-plugin-relay": "^10.1.3",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "~16.13.1",
    "react-native": "0.63.4",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.7",
    "autoprefixer": "^10.0.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "postcss": "^8.0.9",
    "wsrun": "^5.2.4"
  },
  "babel": {
    "plugins": [
      "relay"
    ]
  }
}
